!!!
%html
  %head
    %title Dashboard
    = stylesheet_link_tag 'desktop.css', :media => 'screen, projection'
    = stylesheet_link_tag 'upbar.css', :media => 'screen, projection'
    /[if IE]
      = stylesheet_link_tag 'ie.css', :media => 'screen, projection'
    = javascript_include_tag :defaults
    = csrf_meta_tag
  %body
    = render :partial => 'shared/up_bar'
    = yield
